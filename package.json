{
  "name": "Today!",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Makes the \"Today\" button of Google Calendar more patriotic.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "https://www.google.com/"
  ],
  "web_accessible_resources": [
    "today.wav",
    "neildiamond.png"
  ],
  "dependencies": {
    "express": "^4.10.2",
    "socket.io": "^1.3.7"
  }
}
